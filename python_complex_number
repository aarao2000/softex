
# Crie um tipo abstrato de dado (TAD) para manipular números complexos na linguagem Python.
# O método deve:

# - calcular três números complexos;
# - realizar todas as operações básicas;
# - e imprimir as propriedades real e img do números. 

# inclui "cmath" para operacoes com numeros complexos
import cmath

x = 3.14 + 2j
y = complex (3,4)
z = complex (5,6)

soma = x + y
subt = y - z
mult = x * z
divi = z / y


print ("parte real and imaginaria do numero complexo X: ",end="")
print (x.real, x.imag)

print ("parte real and imaginaria do numero complexo Y: ",end="")
print (y.real, y.imag)

print ("parte real and imaginaria do numero complexo Z: ",end="")
print (z.real, z.imag)
  
print ("parte real and imaginaria dos numeros complexos X+Y: ",end="")
print (soma.real, soma.imag)

print ("parte real and imaginaria dos numeros complexos Y-Z: ",end="")
print (subt.real, subt.imag)

print ("parte real and imaginaria dos numeros complexos X*Z: ",end="")
print (mult.real, mult.imag)

print ("parte real and imaginaria dos numeros complexos Z/Y: ",end="")
print (divi.real, divi.imag)
